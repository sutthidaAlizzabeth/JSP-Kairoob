<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PhotoListServlet</servlet-name>
        <servlet-class>Servlet.PhotoListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCart</servlet-name>
        <servlet-class>Servlet.AddToCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowCartServlet</servlet-name>
        <servlet-class>Servlet.ShowCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckoutServlet</servlet-name>
        <servlet-class>Servlet.CheckoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveFormCart</servlet-name>
        <servlet-class>Servlet.RemoveFormCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotoDetailServlet</servlet-name>
        <servlet-class>Servlet.PhotoDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountShowServlet</servlet-name>
        <servlet-class>Servlet.AccountShowServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountEditServlet</servlet-name>
        <servlet-class>Servlet.AccountEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountEditShowServlet</servlet-name>
        <servlet-class>Servlet.AccountEditShowServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaleOrderAllServlet</servlet-name>
        <servlet-class>Servlet.SaleOrderAllServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryShopping</servlet-name>
        <servlet-class>Servlet.HistoryShoppingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyPhotoServlet</servlet-name>
        <servlet-class>Servlet.MyPhotoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PhotoListServlet</servlet-name>
        <url-pattern>/PhotoList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToCart</servlet-name>
        <url-pattern>/AddToCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowCartServlet</servlet-name>
        <url-pattern>/ShowCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckoutServlet</servlet-name>
        <url-pattern>/Checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveFormCart</servlet-name>
        <url-pattern>/RemoveFormCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotoDetailServlet</servlet-name>
        <url-pattern>/PhotoDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountShowServlet</servlet-name>
        <url-pattern>/AccountShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountEditServlet</servlet-name>
        <url-pattern>/AccountEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountEditShowServlet</servlet-name>
        <url-pattern>/AccountEditShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaleOrderAllServlet</servlet-name>
        <url-pattern>/SaleOrderAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryShopping</servlet-name>
        <url-pattern>/HistoryShopping</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyPhotoServlet</servlet-name>
        <url-pattern>/MyPhoto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
